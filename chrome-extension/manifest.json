{
    "manifest_version": 2,
    "name": "Sentimentext",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
            "<all_urls>"
            ],
            "js": ["source/content-script.js"]
        }
    ],
    "browser_action": {
        "default_icon": {
            "16": "icons/icon16.png"        
        },
        "default_title": "Sentimentext", 
        "default_popup": "source/popup.html"
    }
}